pipeline {
  agent any
    stages {
      stage('Starting ...') {
        echo 'Starting'
      }
    }
    stage('Compiling')
      steps {
        bat 'tools\jdk-18.0.1.1\bin\javac.exe Start.java'
      }
    }
    stage('Testing')
      steps {
        bat 'tools\jdk-18.0.1.1\bin\javac.exe Start'
      }
    }
  }
}
      
